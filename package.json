{
  "name": "projects-switcher",
  "displayName": "Projects Switcher",
  "description": "Switch between project folders while preserving tabs and sessions with intelligent workspace filtering",
  "version": "1.0.4",
  "publisher": "khanhromvn",
  "author": {
    "name": "KhanhRomVN",
    "email": "khanhromvn@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/KhanhRomVN/ProjectSwitcher#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/KhanhRomVN/ProjectSwitcher.git"
  },
  "bugs": {
    "url": "https://github.com/KhanhRomVN/ProjectSwitcher/issues"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "project",
    "workspace",
    "switcher",
    "tabs",
    "session",
    "filtering",
    "workspace-management",
    "folder-filtering",
    "tab-management"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "project-switcher.switchProject",
        "title": "Switch to Project"
      },
      {
        "command": "project-switcher.toggleProjectEnabled",
        "title": "Toggle Project",
        "icon": "$(circle-slash)"
      },
      {
        "command": "project-switcher.enableProject",
        "title": "Enable Project",
        "icon": "$(check-all)"
      },
      {
        "command": "project-switcher.disableProject",
        "title": "Disable Project",
        "icon": "$(circle-slash)"
      },
      {
        "command": "project-switcher.saveSession",
        "title": "Save Session",
        "icon": "$(save)"
      },
      {
        "command": "project-switcher.clearSession",
        "title": "Clear Session",
        "icon": "$(clear-all)"
      },
      {
        "command": "project-switcher.moveUp",
        "title": "Move Up",
        "icon": "$(arrow-up)"
      },
      {
        "command": "project-switcher.moveDown",
        "title": "Move Down", 
        "icon": "$(arrow-down)"
      },
      {
        "command": "project-switcher.showProjectMenu",
        "title": "Project Menu",
        "icon": "$(list-selection)"
      },
      {
        "command": "project-switcher.enable",
        "title": "Enable Project Switcher",
        "icon": "$(folder-library)"
      },
      {
        "command": "project-switcher.disable",
        "title": "Disable Project Switcher",
        "icon": "$(close)"
      },
      {
        "command": "project-switcher.toggleMode",
        "title": "Toggle Project Switcher",
        "icon": "$(settings-gear)"
      },
      {
        "command": "project-switcher.toggleFiltering",
        "title": "Toggle Project Filtering",
        "icon": "$(filter)"
      },
      {
        "command": "project-switcher.openProjectSwitchMenu",
        "title": "Open Project Switch Menu"
      },
      {
        "command": "project-switcher.debugState",
        "title": "Debug: Show Extension State"
      },
      {
        "command": "project-switcher.debugContext",
        "title": "Debug: Check VS Code Contexts"
      },
      {
        "command": "project-switcher.debugCommands",
        "title": "Debug: List Registered Commands"
      },
      {
        "command": "project-switcher.debugActivityBar",
        "title": "Debug: Activity Bar Issues"
      }
    ],
    "keybindings": [
      {
        "command": "project-switcher.switchToProject1",
        "key": "ctrl+alt+1",
        "mac": "cmd+alt+1",
        "when": "config.projectSwitcher.enableKeyboardShortcuts"
      },
      {
        "command": "project-switcher.switchToProject2", 
        "key": "ctrl+alt+2",
        "mac": "cmd+alt+2",
        "when": "config.projectSwitcher.enableKeyboardShortcuts"
      },
      {
        "command": "project-switcher.switchToProject3",
        "key": "ctrl+alt+3",
        "mac": "cmd+alt+3",
        "when": "config.projectSwitcher.enableKeyboardShortcuts"
      },
      {
        "command": "project-switcher.switchToProject4",
        "key": "ctrl+alt+4",
        "mac": "cmd+alt+4",
        "when": "config.projectSwitcher.enableKeyboardShortcuts"
      },
      {
        "command": "project-switcher.switchToProject5",
        "key": "ctrl+alt+5",
        "mac": "cmd+alt+5",
        "when": "config.projectSwitcher.enableKeyboardShortcuts"
      },
      {
        "command": "project-switcher.switchToProject6",
        "key": "ctrl+alt+6",
        "mac": "cmd+alt+6",
        "when": "config.projectSwitcher.enableKeyboardShortches"
      },
      {
        "command": "project-switcher.switchToProject7",
        "key": "ctrl+alt+7",
        "mac": "cmd+alt+7",
        "when": "config.projectSwitcher.enableKeyboardShortcuts"
      },
      {
        "command": "project-switcher.switchToProject8",
        "key": "ctrl+alt+8",
        "mac": "cmd+alt+8",
        "when": "config.projectSwitcher.enableKeyboardShortcuts"
      },
      {
        "command": "project-switcher.switchToProject9",
        "key": "ctrl+alt+9",
        "mac": "cmd+alt+9",
        "when": "config.projectSwitcher.enableKeyboardShortcuts"
      },
      {
        "command": "project-switcher.openProjectSwitchMenu",
        "key": "ctrl+alt+m",
        "mac": "cmd+alt+m",
        "when": "projectSwitcher.isEnabled"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "project-switcher",
          "title": "Project Switcher",
          "icon": "$(folder-library)"
        }
      ]
    },
    "views": {
      "project-switcher": [
        {
          "id": "projectManager",
          "name": "Projects",
          "contextualTitle": "Project Manager"
        }
      ],
      "explorer": [
        {
          "id": "allProjects",
          "name": "ALL PROJECTS",
          "when": "projectSwitcher.isEnabled && projectSwitcher.hasMultipleProjects"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "project-switcher.enableProject",
          "when": "view == projectManager && viewItem == disabledProject",
          "group": "inline@1"
        },
        {
          "command": "project-switcher.disableProject",
          "when": "view == projectManager && viewItem == project",
          "group": "inline@1"
        },
        {
          "command": "project-switcher.moveUp",
          "when": "view == projectManager && viewItem == project",
          "group": "inline@2"
        },
        {
          "command": "project-switcher.moveDown",
          "when": "view == projectManager && viewItem == project", 
          "group": "inline@3"
        },
        {
          "command": "project-switcher.switchProject",
          "when": "view == allProjects && viewItem == allProject",
          "group": "navigation@1"
        }
      ],
      "commandPalette": [
        {
          "command": "project-switcher.switchProject",
          "when": "false"
        },
        {
          "command": "project-switcher.toggleProjectEnabled",
          "when": "false"
        },
        {
          "command": "project-switcher.enableProject",
          "when": "false"
        },
        {
          "command": "project-switcher.disableProject",
          "when": "false"
        },
        {
          "command": "project-switcher.saveSession",
          "when": "false"
        },
        {
          "command": "project-switcher.clearSession",
          "when": "false"
        },
        {
          "command": "project-switcher.moveUp",
          "when": "false"
        },
        {
          "command": "project-switcher.moveDown",
          "when": "false"
        }
      ]
    },
    "configuration": {
      "title": "Project Switcher",
      "properties": {
        "projectSwitcher.preserveTabs": {
          "type": "boolean",
          "default": true,
          "description": "Preserve open tabs when switching projects"
        },
        "projectSwitcher.autoSaveTabs": {
          "type": "boolean",
          "default": true,
          "description": "Automatically save tab state when switching"
        },
        "projectSwitcher.sessionManagementDefault": {
          "type": "boolean",
          "default": true,
          "description": "Enable session management by default for new projects"
        },
        "projectSwitcher.enableFiltering": {
          "type": "boolean",
          "default": true,
          "description": "Enable project filtering by default (show only active project folder)"
        },
        "projectSwitcher.enableKeyboardShortcuts": {
          "type": "boolean",
          "default": true,
          "description": "Enable keyboard shortcuts for quick project switching (Ctrl+Alt+1-9)"
        },
        "projectSwitcher.autoEnableOnStartup": {
          "type": "boolean",
          "default": false,
          "description": "Automatically enable Project Switcher for parent directory workspaces"
        },
        "projectSwitcher.logLevel": {
          "type": "string",
          "enum": ["debug", "info", "warn", "error"],
          "default": "info",
          "description": "Set the logging level for troubleshooting"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch", 
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "test": "vscode-test",
    "clean": "rimraf dist out",
    "build": "npm run clean && npm run compile",
    "build:prod": "npm run clean && npm run package"
  },
  "devDependencies": {
    "@types/vscode": "^1.50.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "ts-loader": "^9.5.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "rimraf": "^3.0.2"
  },
  "extensionDependencies": [],
  "badges": [
    {
      "url": "https://img.shields.io/github/issues/KhanhRomVN/ProjectSwitcher",
      "href": "https://github.com/KhanhRomVN/ProjectSwitcher/issues",
      "description": "Issues"
    },
    {
      "url": "https://img.shields.io/github/stars/KhanhRomVN/ProjectSwitcher",
      "href": "https://github.com/KhanhRomVN/ProjectSwitcher",
      "description": "Stars"
    }
  ],
  "qna": "marketplace"
}